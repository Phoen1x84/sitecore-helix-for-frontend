{
  "name": "sitecore-helix-for-frontend",
  "version": "1.0.0",
  "description": "example of a front-end application for sitecore helix projects",
  "main": "gulpfile.js",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": [
      "<rootDir>/bower_components/",
      "<rootDir>/node_modules/",
      "<rootDir>/obj/"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "sasslintConfig": "./sasslint.config.json",
  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "eqeqeq": "off",
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "one-var": [
        "error",
        "never"
      ],
      "strict": [
        "error",
        "safe"
      ],
      "camelcase": "error",
      "max-depth": [
        "error",
        4
      ]
    },
    "globals": {
      "window": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "env": {
      "es6": true,
      "browser": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phoen1x84/sitecore-helix-for-frontend.git"
  },
  "keywords": [
    "sitecore-helix",
    "sitecore-frontend"
  ],
  "author": "Matthew Neil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phoen1x84/sitecore-helix-for-frontend/issues"
  },
  "homepage": "https://github.com/Phoen1x84/sitecore-helix-for-frontend#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.5.1",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-jest": "^4.0.3",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-uglify": "^3.0.2",
    "jest-cli": "^23.6.0",
    "node-normalize-scss": "^8.0.1",
    "node-sass": "^4.10.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^12.0.5"
  }
}