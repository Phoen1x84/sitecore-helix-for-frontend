{
  "name": "Commercial.Website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": ">2%",
  "scripts": {
    "build": "npm run styles | npm run scripts",
    "dev": "npm run styles:dev | npm run scripts:dev",
    "start": "npm run build && npm run dev",
    "scripts": "rollup -c",
    "scripts:dev": "onchange \"./Scripts/**/*.js\" \"./Views/**/*.js\" -- npm run scripts",
    "styles": "node-sass --importer ../../node_modules/node-sass-glob-importer/dist/cli.js -o ../../dist/ ./Styles/commercial.scss",
    "styles:dev": "onchange \"./Styles/**/*.scss\" \"./Views/**/*.scss\" -- npm run styles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "onchange": "^7.0.2",
    "rollup": "^2.21.0",
    "@rollup/plugin-babel": "^5.1.0"
  }
}